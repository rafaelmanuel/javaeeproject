<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>WEAR-KAPAMPANGAN</display-name>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.wear.kapampangan.project.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>catalog</param-name>
    <param-value>dbwear_kapampangan</param-value>
  </context-param>
  <context-param>
    <param-name>dbuser</param-name>
    <param-value>root</param-value>
  </context-param>
  <context-param>
    <param-name>dbpass</param-name>
    <param-value></param-value>
  </context-param>
  <context-param>
    <description>Location to store uploaded file</description>
    <param-name>file-upload</param-name>
    <param-value>
         c:\apache-tomcat-5.5.29\webapps\data\
     </param-value>
  </context-param>
  <error-page>
    <error-code>404</error-code>
    <location>/wearkapampangan/wear/team/kapampangan/error-page.jsp</location>
  </error-page>
</web-app>